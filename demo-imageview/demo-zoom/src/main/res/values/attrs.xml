<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">





    <declare-styleable name="CircleLayout">
        <attr name="isRotating" format="boolean" />
        <attr name="radius" format="dimension" />
        <attr name="speed" format="integer" />
        <attr name="bgColor" format="reference" />

        <attr name="firstChildPosition">
            <enum name="East" value="0" />
            <enum name="South" value="90" />
            <enum name="West" value="180" />
            <enum name="North" value="270" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CoordinateSystemView">
        <attr name="originOffsetRatio" format="float" />
        <attr name="unitLengthRatio" format="float" />
    </declare-styleable>







</resources>