<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".interpolator.CircleLayoutFragment">

    <com.dafay.demo.lib.base.ui.widgets.SquareFrameLayout
        android:id="@+id/sfl_circle_hour"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-100dp"
        android:scaleX="1.34"
        android:scaleY="1.34">

        <com.dafay.demo.zoom.ui.page.interpolator.CircleLayout
            android:id="@+id/cl_circle_layout_hour"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="40dp"
            app:bgColor="@color/color_aa333145">

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="0"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="1"
                android:textColor="@color/white" />


            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="2"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="3"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="4"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="5"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="6"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="7"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="8"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="9"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="10"
                android:textColor="@color/white" />

            <TextView
                style="@style/Widget.InlineActionButton"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:gravity="center"
                android:text="11"
                android:textColor="@color/white" />


        </com.dafay.demo.zoom.ui.page.interpolator.CircleLayout>

    </com.dafay.demo.lib.base.ui.widgets.SquareFrameLayout>


</FrameLayout>